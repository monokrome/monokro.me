{
  "locals": {
    "name": "monokro.me",
    "owner": "Brandon R. Stoner"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "./node_modules/wintersmith-coffee/",
    "./node_modules/wintersmith-stylus/",
    "./node_modules/wintersmith-browserify/",
    "./node_modules/wintersmith-livereload/"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3
  },
  "browserify": {
    "transforms": [
      "coffeeify",
      "browserify-jade"
    ]
  }
}
