{
  "locals": {
    "name": "monokro.me",
    "owner": "Brandon R. Stoner"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "./node_modules/wintersmith-coffee/",
    "./node_modules/wintersmith-stylus/",
    "./node_modules/wintersmith-jsbundler/"
  ],
