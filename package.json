{
  "name": "webpack-react-starter",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "development": "NODE_ENV=development npm run services",
    "production": "NODE_PATH=./lib NODE_ENV=production node lib/services",
    "services": "NODE_PATH=./src nodemon -d 3 -w package.json -w .babelrc -w webpack.config.js -e js -e jsx -x babel-node src/services",
    "build": "NODE_ENV=production webpack -p && babel src -d lib",
    "precommit": "npm run lint",
    "start": "NODE_ENV=development npm run services",
    "frontend": "NODE_ENV=development webpack-dev-server --hot --inline",
    "lint": "flow-typed install > /dev/null && prettier --no-color --trailing-comma all --no-semi --single-quote --write \"src/**/*.js\" \"src/**/*.jsx\" && eslint --fix src/services && flow",
    "test": "jest",
    "tests": "jest --watch --notify",
    "develop": "npm run development"
  },
  "author": "Bailey Stoner <monokrome@monokro.me>",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "express": "^4.15.3",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.0.0",
    "nodemon": "^1.11.0",
    "prettier-eslint-cli": "^4.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "recompose": "^0.25.0",
    "styled-components": "^2.0.0",
    "url-loader": "^0.5.9"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-loader": "^0.1.0",
    "file-loader": "^0.11.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.47.0",
    "flow-typed": "^2.1.2",
    "glob": "^7.1.2",
    "html-loader": "^0.4.5",
    "jest": "^20.0.4",
    "pleeease-filters": "^4.0.0",
    "prettier": "^1.4.2",
    "prop-types": "^15.5.10",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/fixtures/static/style.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/fixtures/static/style.js"
    }
  }
}
