each article in articles
  article.article.intro
    header
      .col-xs-8
        h2: a(href=article.url, target='_self')= article.title

      .col-xs-4
        p.date= moment(article.date).format('MM/DD/YYYY')

    section!=typogr(article.intro).typogrify()

    if article.hasMore
      p.more
        a(href=article.url, target='_self') more
